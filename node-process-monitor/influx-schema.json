{
  "objectStore": "file",
  "dataDir": "./influxdb/data",
  "tables": [
    {
      "database": "process-monitor",
      "name": "focused",
      "tags": ["title","process", "path", "url"]
    },
    {
      "database": "process-monitor",
      "name": "process",
      "tags": ["name", "pid", "status", "started"],
      "fields": {
        "cpu": "float64",
        "memory": "int64",
        "unresponsive": "bool"
      }
    },
    {
      "database": "process-monitor",
      "name": "system",
      "tags": ["sleepState"],
      "fields": {
        "isIdle": "bool"
      }
    }
  ]
}
